        </el-table-column>
        <el-table-column prop="CE" label="CE">
        </el-table-column>
        <el-table-column prop="EMC" label="EMC">
        </el-table-column>
        <el-table-column prop="BIFMA" label="BIFMA">
        </el-table-column>
