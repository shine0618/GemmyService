                    </el-col>
                </el-form-item>

                <el-form-item prop="retrievecode">
                    <span slot="label">
                        <span>{{NAVForgetCode}}</span>
                        <i class="el-icon-message"></i>
                    </span>
                    <el-input v-model="ruleRetrieve.retrievecode" autocomplete="off"></el-input>
                </el-form-item>
                <el-form-item prop="retrievePassword">
                    <span slot="label">
                        <span>{{NAVForgetNewPassword}}</span>
                        <i class="el-icon-lock"></i>
                    </span>
                    <el-input type="password" v-model="ruleRetrieve.retrievePassword" autocomplete="off"></el-input>
                </el-form-item>
                <el-form-item prop="retrievecheckPassword">
                    <span slot="label">
                        <span>{{NAVForgetConfirmPassword}}</span>
                        <i class="el-icon-key"></i>
                    </span>
                    <el-input type="password" v-model="ruleRetrieve.retrievecheckPassword" autocomplete="off"></el-input>
                </el-form-item>


                <el-form-item>
                    <button type="button" class="thm-btn orderbtn" style="width:100%" @@click="reset(ruleRetrieve.retrieveUsername,ruleRetrieve.retrievePassword,ruleRetrieve.retrievecode)"> {{NAVForgetResetButton}}</button>

                    @*<el-col :span="12" align="center">
                            <el-button type="primary" @@click="reset(ruleRetrieve.retrieveUsername,ruleRetrieve.retrievePassword,ruleRetrieve.retrievecode)">重置</el-button>


                        </el-col>
                        <el-col :span="11" align="left">
                            <el-button @@click="resetForm('ruleForm')">清空</el-button>
                        </el-col>*@
                </el-form-item>
            </el-form>
        </el-dialog>

        <el-dialog :title="NAVRegisterUseNotice"
                   :visible.sync="dialogVisible_use"
                   width="600px">
            <p class="explainText">
                {{NAVRegisterUseContent}}
            </p>
        </el-dialog>
