

<style>
    .loginPersonBox {
        cursor: pointer;
    }

    .drawerh5 {
        color: var(--thm-base);
    }

    .explainText {
        font-size: 13px;
    }


    .drag {
        border-radius: 30px;
        position: relative;
        background-color: #75CDF9;
        width: 300px;
        height: 34px;
        margin-left: 0px;
        margin-top: 0px;
        line-height: 34px;
        text-align: center;
    }

    .handler {
        border-radius: 30px;
        position: absolute;
        top: 0px;
        left: 0px;
        width: 40px;
        height: 32px;
        border: 1px solid #ccc;
        cursor: move;
    }

    .handler_bg {
        background: #fff url("data:image/png;base64,") no-repeat center;
    }

    .handler_ok_bg {
        background: #fff url("data:image/png;base64,") no-repeat center;
    }

    .drag_bg {
        border-radius: 30px;
        background-color: #13CE66;
        height: 34px;
        width: 0px;
    }

    .drag_text {
        position: absolute;
        top: 0px;
        width: 300px;
        -moz-user-select: none;
        -webkit-user-select: none;
        user-select: none;
        -ms-user-select: none;
    }

    .el-col-24 {
        width: 34px;
    }

    .el-avatar {
        width: 100px;
        height: 100px;
        line-height: 100px;
        font-size: 54px;
    }

    .Personal_span {
        font-size: 16px;
        padding: 6px;
    }

    .contact_div {
        margin-top: 25px;
    }
</style>


<div id="nav_langu_box">
    <div>
        <el-drawer title="注册"
                   show-close="true"
                   :visible.sync="drawer"
                   :with-header="false"
                   size="520px">
            <div class="container" style="padding:3rem">

                @if (@Session["configurationPerson"] == null)
                {
                <h4 class="drawerh5">{{NAVLogin}}</h4>
                    <el-form :model="ruleLoginForm" status-icon :rules="rules" ref="ruleLoginForm" label-width="100px" class="demo-ruleForm">
                        <el-form-item prop="username">
                            <span slot="label">
                                <span>{{NAVLoginAccount}}</span>
                                <i class="el-icon-user"></i>
                            </span>
                            <el-input type="text" v-model="ruleLoginForm.username"></el-input>
                        </el-form-item>
                        <el-form-item prop="pass">
                            <span slot="label">
                                <span>{{NAVLoginPassword}}</span>
                                <i class="el-icon-lock"></i>
                            </span>
                            <el-input type="password" v-model="ruleLoginForm.pass" autocomplete="off"></el-input>
                        </el-form-item>
                        <el-form-item v-show="isLoginCheck">
                            <span slot="label">
                                <span>{{NAVLoginCode}}</span>
                                <i class="el-icon-message"></i>
                            </span>
                            <el-col :span="18">
                                <div class="drag">
                                    <div class="drag_bg"></div>
                                    <div class="drag_text">{{confirmWords}}</div>
                                    <div @@mousedown="mousedownFn($event)" class="handler handler_bg"></div>
                                </div>
                            </el-col>


                        </el-form-item>


                        <el-form-item>
                            @*<el-button v-on:click="login(ruleLoginForm.username,ruleLoginForm.pass,confirmSuccess,isLoginCheck)">登录</el-button>*@
                            <button type="button" class="thm-btn orderbtn" style="width:100%" @@click="login(ruleLoginForm.username,ruleLoginForm.pass,confirmSuccess,isLoginCheck)"> {{NAVLoginButton}}</button>
                        </el-form-item>

                        <el-form-item>
                            <el-col :span="22" align="right"> <el-link type="primary" v-on:click="dialogVisible_forget=true">{{NAVLoginForgetLink}}</el-link></el-col>
                        </el-form-item>


                    </el-form>
                    <h4 class="drawerh5">{{NAVRegister}}</h4>
                    <p class="explainText">{{NAVRegisterDesc}}</p>
                    <el-form :model="ruleRegisterForm" status-icon :rules="rulesRegister" ref="ruleRegisterForm" label-width="100px" class="demo-ruleForm">
                        <el-form-item prop="registerusername">
                            <span slot="label">
                                <span>{{NAVRegisterAccount}}</span>
                                <i class="el-icon-user"></i>
                            </span>
                            <el-input type="text" v-model="ruleRegisterForm.registerusername" autocomplete="off"></el-input>
                        </el-form-item>
                        <el-form-item prop="registercode">
                            <span slot="label">
                                <span>{{NAVRegisterCode}}</span>
                                <i class="el-icon-postcard"></i>
                            </span>
                            <el-col :span="16"><el-input type="text" v-model="ruleRegisterForm.registercode" autocomplete="new-password"></el-input></el-col>
                            <el-col :span="6" align="right">
                                <el-button v-on:click="sendEmail(ruleRegisterForm.registerusername)">
                                    <span v-show="registershow">{{NAVRegisterSendEmail}}</span>
                                    <span v-show="!registershow">   {{count}} s</span>
                                </el-button>
                        </el-form-item>
                        <el-form-item prop="registername">
                            <span slot="label">
                                <span>{{NAVRegisterFirstName}}</span>
                                <i class="el-icon-postcard"></i>
                            </span>
                            <el-col :span="9"><el-input type="text" v-model="ruleRegisterForm.registerfirstname" autocomplete="off"></el-input></el-col>
                            <el-col :span="5" align="center">
                                <span>{{NAVRegisterLastName}}</span>
                                <i class="el-icon-postcard"></i>
                            </el-col>
                            <el-col :span="10"><el-input type="text" v-model="ruleRegisterForm.registerlastname" autocomplete="off"></el-input></el-col>
                        </el-form-item>
                        <el-form-item prop="registerpass">
                            <span slot="label">
                                <span>{{NAVRegisterPassword}}</span>
                                <i class="el-icon-lock"></i>
                            </span>
                            <el-input type="password" v-model="ruleRegisterForm.registerpass" autocomplete="off"></el-input>
                        </el-form-item>
                        <el-form-item prop="registercheckPass">
                            <span slot="label">
                                <span>{{NAVRegisterConfirmPassword}}</span>
                                <i class="el-icon-key"></i>
                            </span>
                            <el-input type="password" v-model="ruleRegisterForm.registercheckPass" autocomplete="off"></el-input>
                        </el-form-item>

                        <el-form-item>
                            @*    <el-button class="thm-btn orderbtn" v-on:click="register(ruleRegisterForm.registerusername,ruleRegisterForm.registerpass,ruleRegisterForm.registercode,ruleRegisterForm.registercheckPass,ruleRegisterForm.registerfirstname,ruleRegisterForm.registerlastname)">发送邮件</el-button>*@
                            <button type="button" class="thm-btn orderbtn" style="width:100%" @@click="register(ruleRegisterForm.registerusername,ruleRegisterForm.registerpass,ruleRegisterForm.registercode,ruleRegisterForm.registercheckPass,ruleRegisterForm.registerfirstname,ruleRegisterForm.registerlastname)"> {{NAVRegisterButton}}</button>
                        </el-form-item>
                        <el-form-item>
                            <el-col :span="22" align="right"> <el-link type="primary" v-on:click="dialogVisible_use=true">{{NAVRegisterNotice}}</el-link></el-col>
                        </el-form-item>
                    </el-form>
                }
                else
                {
                    <el-row>
                        <el-col :span="8"><el-avatar style="background:var(--thm-base)">@Session["picName"] </el-avatar></el-col>
                        <h3>@Session["configurationPerson"]</h3>                        
                        <button type="button" class="thm-btn orderbtn" style="width:50%" @@click="LoginOut"> {{NAVLoginQuit}}</button>

                    </el-row>



                    <template>
                        <el-collapse v-model="activeNames" style="font-size:17px;" accordion>
                            <el-collapse-item :title="NAVLoginPersonInfo" name="1">
                                <div v-for="item in infoDataForm1" class="Personal_span">
                                    <i :class="item.icon" style="font-size:20px;color: var(--thm-base)"></i>
                                    <span>
                                        {{ item.menuName }}
                                        <br />
                                    </span>
                                </div>

                            </el-collapse-item>
                            <el-collapse-item :title="NAVLoginCompanyInfo" name="2">

                                <div v-for="item in infoDataForm2" class="Personal_span">
                                    <i :class="item.icon" style="font-size:20px;color: var(--thm-base)"></i>
                                    <span>
                                        {{ item.menuName }}
                                        <br />
                                    </span>
                                </div>



                            </el-collapse-item>
                            <el-collapse-item :title="NAVLoginSetting"  name="3">
                                <a href="/JCSelection/PersonInfo?email=@Session["emailName"]&name=@Session["configurationPerson"]" class="Personal_span"><i class="el-icon-s-tools" style="font-size:20px;color: var(--thm-base)"></i><span style="color:#303133">{{NAVLoginPersonSetting}}</span></a>

                            </el-collapse-item>
                            <el-collapse-item :title="NAVLoginResetPassword" name="4">
                                <el-form :model="resetPasswordLogin" status-icon :rules="rulesresetPasswordLogin" ref="resetPasswordLogin" label-width="100px" class="demo-ruleForm">
                                    <el-form-item prop="oldPassword">
                                        <span slot="label">
                                            <span>{{NAVLoginResetOldPassword}}</span>
                                            <i class="el-icon-lock" style="font-size:20px;color: var(--thm-base)"></i>
                                        </span>
                                        <el-input type="password" v-model="resetPasswordLogin.oldPassword" autocomplete="new-password"></el-input>
                                    </el-form-item>
                                    <el-form-item prop="newPassword">
                                        <span slot="label">
                                            <span>{{NAVLoginResetNewPassword}}</span>
                                            <i class="el-icon-lock" style="font-size:20px;color: var(--thm-base)"></i>
                                        </span>
                                        <el-input type="password" v-model="resetPasswordLogin.newPassword" autocomplete="new-password"></el-input>
                                    </el-form-item>
                                    <el-form-item prop="checkNewPassword">
                                        <span slot="label">
                                            <span>{{NAVLoginResetConfirmPassword}}</span>
                                            <i class="el-icon-lock" style="font-size:20px;color: var(--thm-base)"></i>
                                        </span>
                                        <el-input type="password" v-model="resetPasswordLogin.checkNewPassword" autocomplete="new-password"></el-input>
                                    </el-form-item>
                                    <el-form-item>
                                        <button type="button" class="thm-btn orderbtn" style="width:100%" @@click="resetLogin(loginEmail,resetPasswordLogin.oldPassword,resetPasswordLogin.newPassword)"> {{NAVLoginResetButton}}</button>
                                    </el-form-item>
                                </el-form>
                            </el-collapse-item>
                        </el-collapse>
                    </template>


                    <br />
                    <div class="contact_div">
                        <h6>{{NAVLoginQuestionContact}}</h6>

                        <div class="row">
                            <div class="col-lg-4 col-xs-4">
                                <el-avatar src="https://cube.elemecdn.com/0/88/03b0d39583f48206768a7534e55bcpng.png"></el-avatar>
                            </div>
                            <div class="col-xs-8">
                                <h3 style="margin-top:33px;">Gemmy</h3>
                            </div>

                        </div>



                        <div v-for="item in infoDataForm_contact" class="Personal_span">
                            <i :class="item.icon" style="font-size:20px;color: var(--thm-base)"></i>
                            <span>
                                {{ item.menuName }}
                                <br />
                            </span>
                        </div>
                    </div>


                }




            </div>


        </el-drawer>
        <el-dialog :title="NAVForgetTitle"
                   :visible.sync="dialogVisible_forget"
                   width="600px">
            <el-form :model="ruleRetrieve" status-icon :rules="rulesRetrieve" ref="ruleRetrieve" label-width="100px" class="demo-ruleForm">
